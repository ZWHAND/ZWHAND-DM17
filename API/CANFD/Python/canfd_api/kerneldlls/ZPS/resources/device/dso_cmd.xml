<cmds>
	<!--DSO业务-->
	<cmd id="SetDsoSource"   	param="INT32:NUM,DSO_SOURCE_TYPE:A" 		send_str=":DSO:CHANnel{NUM}:SOURce {A}" ch="ch0"/>
	<cmd id="GetDsoSource"   	param="INT32:NUM,DSO_SOURCE_TYPE&amp;:A" send_str=":DSO:CHANnel{NUM}:SOURce?" 	    recv_str="{A}"  ch="ch0"/>
    <cmd id="SetDsoScale"   	param="INT32:NUM,FLOAT:A" 		send_str=":DSO:CHANnel{NUM}:SCALe {A}" ch="ch0"/>
	<cmd id="GetDsoScale"   	param="INT32:NUM,FLOAT&amp;:A" send_str=":DSO:CHANnel{NUM}:SCALe?" 	    recv_str="{A}"  ch="ch0"/>
    <cmd id="SetDsoOffset"   	param="INT32:NUM,FLOAT:A" 		send_str=":DSO:CHANnel{NUM}:OFFSet {A}" ch="ch0"/>
	<cmd id="GetDsoOffset"   	param="INT32:NUM,FLOAT&amp;:A" send_str=":DSO:CHANnel{NUM}:OFFSet?" 	    recv_str="{A}"  ch="ch0"/>
    <cmd id="SetDsoCoupling"   	param="INT32:NUM,DSO_COUPLING_TYPE:A" 		send_str=":DSO:CHANnel{NUM}:COUPling  {A}" ch="ch0"/>
	<cmd id="GetDsoCoupling"   	param="INT32:NUM,DSO_COUPLING_TYPE&amp;:A" send_str=":DSO:CHANnel{NUM}:COUPling ?" 	    recv_str="{A}"  ch="ch0"/>
    <cmd id="SetDsoUnits"   	param="INT32:NUM,DSO_UNITS_TYPE:A" 		send_str=":DSO:CHANnel{NUM}:UNITs {A}" ch="ch0"/>
	<cmd id="GetDsoUnits"   	param="INT32:NUM,DSO_UNITS_TYPE&amp;:A" send_str=":DSO:CHANnel{NUM}:UNITs?" 	    recv_str="{A}"  ch="ch0"/>
    <cmd id="SetDsoProbe"   	param="INT32:NUM,FLOAT:A" 		send_str=":DSO:CHANnel{NUM}:PROBe  {A}" ch="ch0"/>
	<cmd id="GetDsoProbe"   	param="INT32:NUM,FLOAT&amp;:A" send_str=":DSO:CHANnel{NUM}:PROBe ?" 	    recv_str="{A}"  ch="ch0"/>
    <cmd id="SetDsoFilter"   	param="INT32:NUM,DSO_FILTER_TYPE:A" 		send_str=":DSO:CHANnel{NUM}:Filter {A}" ch="ch0"/>
	<cmd id="GetDsoFilter"   	param="INT32:NUM,DSO_FILTER_TYPE&amp;:A" send_str=":DSO:CHANnel{NUM}:Filter?" 	    recv_str="{A}"  ch="ch0"/>
	<cmd id="GetDsoOffsetScope"   	param="INT32:NUM,DSO_FILTER_TYPE&amp;:A" send_str=":DSO:CHANnel{NUM}:OFFSet:SCOPe?" 	    recv_str="{A}"  ch="ch0"/>

	<cmd id="SetDsoSource"   	param="INT32:NUM,DSO_SOURCE_TYPE:A" 		send_str=":DSO:CHANnel{NUM}:SOURce {A}" ch="ch0"/>
	<cmd id="GetDsoSource"   	param="INT32:NUM,DSO_SOURCE_TYPE&amp;:A" send_str=":DSO:CHANnel{NUM}:SOURce?" 	    recv_str="{A}"  ch="ch0"/>

	
	<cmd id="SetAIChannelStop" 	    param="INT32:NUM" 		send_str=":AI:CHANnel{NUM}:STOP" ch="ch0"/>
	<cmd id="SetAIChannelScope"   	param="INT32:NUM,STRING:A" 		send_str=":AI:CHANnel{NUM}:SCOPe {A}" ch="ch0"/>
	<cmd id="GetAIChannelScope"   	param="INT32:NUM,STRING&amp;:A" send_str=":AI:CHANnel{NUM}:SCOPe?" 	    recv_str="{A}"  ch="ch0"/>
	<cmd id="GetAIChannelData"   	param="INT32:NUM,FLOAT&amp;:A" send_str=":AI:CHANnel{NUM}:DATA?" 	    recv_str="{A}"  ch="ch0"/>
	<cmd id="SetAISampleRate"   	param="FLOAT:A" send_str=":AI:SAMPrate {A}" ch="ch0"/>
	<cmd id="GetAISampleRate"   	param="FLOAT&amp;:A" send_str=":AI:SAMPrate?" 	    recv_str="{A}"  ch="ch0"/>
	<cmd id="SetAOChannelBindData"   	param="INT32:NUM,IBASECTR:A" 		send_str=":AO:CHANnel{NUM}:BINData {b:A}" ch="ch0" comment="{b:A}发的不带@"/>
	<cmd id="SetAOChannelRawData"   	param="INT32:NUM,INT32:A" 		send_str=":AO:CHANnel{NUM}:RAWData  {A}" ch="ch0"/>
	<cmd id="SetAOConfig"  param="INT32:NUM,aoParaConfig:A" send_str=":AO:CHANnel{NUM}:STRUct:CONFig ${A}$" ch="ch0"/>
	<cmd id="GetAOConfig"  param="INT32:NUM,aoParaConfig&amp;:A" send_str=":AO:CHANnel{NUM}:STRUct:CONFig?" 	recv_str="{b:@A}"  ch="ch0" comment="{b:A}收的带@"/>
	<cmd id="GetAIMemoryLength"   		param="INT32:NUM,INT32&amp;:A" send_str=":AI:CHANnel{NUM}:MEMory:LENgth?" 	    recv_str="{A}"  ch="ch0"/>
	<cmd id="GetAIMemorySampRate"   	param="INT32:NUM,FLOAT&amp;:A" send_str=":AI:CHANnel{NUM}:MEMory:SAMPrate?" 	    recv_str="{A}"  ch="ch0"/>
	<cmd id="GetAIMemoryData"   		param="INT32:NUM,INT32:COUNT,UINT8_POINT&amp;:A" send_str=":AI:CHANnel{NUM}:MEMory:DATA? {COUNT}" 	    recv_str="{b:@A}"  ch="ch0"/>
	<cmd id="GetAIMemoryTimeStamp"   	param="INT32:NUM,UINT64&amp;:A" send_str=":AI:CHANnel{NUM}:MEMory:TIMEStamp?" 	    recv_str="{A}"  ch="ch0"/>
</cmds>